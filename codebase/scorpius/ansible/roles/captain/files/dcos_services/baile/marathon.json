{
  "id": "baile",
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "${DOCKER_REGISTRY_PREFIX}knoldususa/baile:${DOCKER_IMAGE_VERSION}",
      "forcePullImage": ${FORCE_PULL_DOCKER},
      "network": "BRIDGE",
      "portMappings": [
        { "hostPort": 0,
          "containerPort": 9000,
          "labels": {
            "VIP_0": "/baile:9000"
          }
        },
        { "hostPort": 0,
          "containerPort": 9001,
          "labels": {
            "VIP_0": "/baile:9001"
            }
        },
        { "hostPort": 0,
          "containerPort": 2552
        }
      ]
    }
  },
  "env": {
    "MONGODB_URL": "${MONGODB_URL}",
    "MONGODB_DB_NAME": "${MONGODB_DB_NAME}",
    "UM_CLIENT_ID": "${UM_CLIENT_ID}",
    "UM_CLIENT_SECRET": "${UM_CLIENT_SECRET}",
    "UM_SERVICE_ORG_ID": "${UM_SERVICE_ORG_ID}",
    "UM_SERVICE_URL": "${UM_SERVICE_URL}",
    "UM_TIMEOUT": "${UM_TIMEOUT}",
    "UM_USER_TOKEN_TTL": "${UM_USER_TOKEN_TTL}",
    "SRV_INSTANCES": "${SRV_INSTANCES}",
    "SRV_CPUS": "${SRV_CPUS}",
    "SRV_MEM": "${SRV_MEM}",
    "AWS_S3_BUCKET": "${AWS_S3_BUCKET}",
    "AWS_REGION": "${AWS_REGION}",
    "AWS_ACCESS_KEY": "${BAILE_AWS_ACCESS_KEY}",
    "AWS_SECRET_KEY": "${BAILE_AWS_SECRET_KEY}",
    "CORTEX_URL": "${CORTEX_URL}",
    "CORTEX_USER": "${CORTEX_USER}",
    "ARIES_URL": "${ARIES_URL}",
    "ARIES_USER": "${ARIES_USER}",
    "SIGNUP_BASE_URL": "${SIGNUP_BASE_URL}",
    "WEB_APP_URL": "${WEB_APP_URL}",
    "BASE_APP_URL": "${BASE_APP_URL}",
    "LOGIN_URL": "${LOGIN_URL}",
    "REQUIRE_EMAIL_CONFIRMATION": "${REQUIRE_EMAIL_CONFIRMATION}",
    "ADMIN_ROLE_ID": "${ADMIN_ROLE_ID}",
    "GEMINI_URL": "${GEMINI_REST_URL}",
    "GEMINI_USER": "${GEMINI_USER}",
    "INTERNAL_API_USERNAME": "${INTERNAL_API_USERNAME}",
    "JUPYTER_SESSION_NODE_CPUS": "${JUPYTER_SESSION_NODE_CPUS}",
    "JUPYTER_SESSION_NODE_GPUS": "${JUPYTER_SESSION_NODE_GPUS}",
    "PREDEF_BUCKET_0_NAME": "${PREDEF_BUCKET_0_NAME}",
    "PREDEF_BUCKET_0_REGION": "${PREDEF_BUCKET_0_REGION}",
    "PREDEF_BUCKET_1_NAME": "${PREDEF_BUCKET_1_NAME}",
    "PREDEF_BUCKET_1_ACCESS_KEY": "${PREDEF_BUCKET_1_ACCESS_KEY}",
    "PREDEF_BUCKET_1_SECRET_KEY": "${PREDEF_BUCKET_1_SECRET_KEY}",
    "PREDEF_BUCKET_1_REGION": "${PREDEF_BUCKET_1_REGION}",
    "MAILER_HOSTNAME": "${MAILER_HOSTNAME}",
    "MAILER_PORT": "${MAILER_PORT}",
    "MAILER_SENDER_EMAIL": "${MAILER_SENDER_EMAIL}",
    "MAILER_SENDER_NAME": "${MAILER_SENDER_NAME}",
    "MAILER_USERNAME": "${MAILER_USERNAME}",
    "MAILER_PASSWORD": "${MAILER_PASSWORD}",
    "ONLINE_PREDICTION_USERNAME": "${ONLINE_PREDICTION_USERNAME}",
    "ONLINE_PREDICTION_STREAM_ID": "${ONLINE_PREDICTION_STREAM_ID}",
    "DEBUG_EXCEPTIONS": "${DEBUG_EXCEPTIONS}",
    "PREDEF_BUCKET_0_ACCESS_KEY": "${PREDEF_BUCKET_0_ACCESS_KEY}",
    "PREDEF_BUCKET_0_SECRET_KEY": "${PREDEF_BUCKET_0_SECRET_KEY}",
    "TEMPORARY_CREDENTIALS_ROLE_ARN": "${TEMPORARY_CREDENTIALS_ROLE_ARN}",
    "HTTP_SERVER_PREFIX": "${HTTP_SERVER_PREFIX}",
    "PRIVATE_HTTP_SERVER_PREFIX": "${PRIVATE_HTTP_SERVER_PREFIX}",
    "ZOOKEEPER_URL": "${ZOOKEEPER_URL}",
    "READ_STORED_PROCESSES": "${READ_STORED_PROCESSES}",
    "AWS_ARN_PARTITION": "${AWS_ARN_PARTITION}",
    "TABLE_STORAGE_USERNAME": "${TABLE_STORAGE_USERNAME}",
    "TABLE_STORAGE_HOST": "${TABLE_STORAGE_HOST}",
    "TABLE_STORAGE_DATABASE": "${TABLE_STORAGE_DATABASE}",
    "TABLE_STORAGE_PORT": "${TABLE_STORAGE_PORT}",
    "TABLE_STORAGE_PROTOCOL": "${TABLE_STORAGE_PROTOCOL}",
    "TABLE_STORAGE_PASSWORD": {
      "secret": "redshift_password"
    },
    "ARIES_PASSWORD": {
      "secret": "aries_http_search_user_password"
    },
    "CORTEX_PASSWORD": {
      "secret": "cortex_http_search_user_password"
    },
    "ONLINE_PREDICTION_PASSWORD": {
      "secret": "baile_online_prediction_password"
    },
    "GEMINI_PASSWORD": {
      "secret": "gemini_password"
    },
    "INTERNAL_API_PASSWORD": {
      "secret": "baile_http_auth_user_password"
    }
},
  "instances": ${SRV_INSTANCES},
  "cpus": ${SRV_CPUS},
  "mem": ${SRV_MEM},
  "constraints": [
      [
        "hostname",
        "UNIQUE"
      ],
      [
        "cluster",
        "UNLIKE",
        "gpu"
      ],
      [
        "cluster",
        "UNLIKE",
        "gpu-jupyter"
      ],
      [
        "cluster",
        "UNLIKE",
        "cpu-jupyter"
      ]
    ],
  "healthChecks": [{
    "protocol": "HTTP",
    "path": "/${HTTP_SERVER_PREFIX}/health",
    "portIndex": 0,
    "timeoutSeconds": 10,
    "gracePeriodSeconds": 30,
    "intervalSeconds": 2,
    "maxConsecutiveFailures": 10
  }],
  "upgradeStrategy": {
    "maximumOverCapacity": 1,
    "minimumHealthCapacity": 0.5
  },
  "secrets": {
    "aries_http_search_user_password": {
      "source": "aries_http_search_user_password"
    },
    "cortex_http_search_user_password": {
      "source": "cortex_http_search_user_password"
    },
    "baile_online_prediction_password": {
      "source": "baile_online_prediction_password"
    },
    "gemini_password": {
      "source": "gemini_http_auth_password"
    },
    "baile_http_auth_user_password": {
      "source": "baile_http_auth_user_password"
    },
    "redshift_password": {
      "source": "redshift_password"
    }
  }
}
