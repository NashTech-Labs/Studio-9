{
  "id": "docker-registry",
  "container": {
    "type": "DOCKER",
    "volumes": [
      {
        "persistent": {
          "type": "root",
          "size": 10000
        },
        "mode": "RW",
        "containerPath": "registrydata"
      },
      {
        "containerPath": "/var/lib/registry",
        "hostPath": "registrydata",
        "mode": "RW"
      }
    ],
    "docker": {
      "image": "registry:2",
      "network": "BRIDGE",
      "portMappings": [
        { "hostPort": 0,
          "containerPort": 5000,
          "labels": {
            "VIP_0": "/docker-registry:80"
          }
        }
      ]
    }
  },
  "instances": 1,
  "cpus": 1,
  "mem": 1024,
  "constraints": [
      [
        "cluster",
        "UNLIKE",
        "gpu"
      ],
      [
        "cluster",
        "UNLIKE",
        "gpu-jupyter"
      ],
      [
        "cluster",
        "UNLIKE",
        "cpu-jupyter"
      ]
    ],
  "healthChecks": [{
    "protocol": "HTTP",
    "path": "/v2/_catalog",
    "portIndex": 0,
    "timeoutSeconds": 10,
    "gracePeriodSeconds": 10,
    "intervalSeconds": 2,
    "maxConsecutiveFailures": 10
  }],
  "upgradeStrategy": {
    "maximumOverCapacity": 0,
    "minimumHealthCapacity": 0
  },
  "unreachableStrategy": "disabled"
}