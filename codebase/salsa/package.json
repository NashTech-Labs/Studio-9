    "@types/jqueryui": "~1.11.37",
    "@types/lodash": "~4.14.85",
    "@types/lokijs": "~1.5.0",
    "@types/topojson": "~3.0.2",
    "ace-builds": "^1.4.3",
    "bootstrap-multiselect": "~0.9.13-1",
    "bootstrap-sass": "3.3.7",
    "cesium": "^1.61.0",
