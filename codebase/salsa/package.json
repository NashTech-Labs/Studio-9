{
  "name": "salsa",
  "version": "2.2.0",
  "homepage": "https://knoldus.com",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:prod": "ng serve --env=prod",
    "start:mock": "ng serve --env=mock",
    "start:falcon": "ng serve --env=falcon",
    "build": "./git-version.js && ng build --env dev --stats-json true",
    "build:mock": "ng build --env=mock --stats-json --output-hashing all",
    "build:prod": "./git-version.js && ng build --env=prod --sourcemaps false --extract-css true --output-hashing all",
    "build:prod:mock": "yarn run build:mock",
    "build:falcon": "./git-version.js && ng build --env=falcon --sourcemaps false --extract-css true --output-hashing all",
    "pretest": "yarn run lint",
    "test": "ng test --code-coverage --port `./pick-port.js`",
    "lint": "ng lint && yarn run swagger-validate",
    "jenkins-checkstyle": "tslint -t checkstyle -o checkstyle.xml 'src/**/*.ts'",
    "server": "http-server dist -a localhost --cors -o",
    "e2e": "ng e2e",
    "ci": "yarn run test",
    "bundle-analyzer": "webpack-bundle-analyzer dist/stats.json",
    "swagger": "swagger-tools",
    "swagger-validate": "swagger-tools validate docs/swagger.yaml"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~5.0.3",
    "@angular/common": "~5.0.3",
    "@angular/compiler": "~5.0.3",
